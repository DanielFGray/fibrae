      "import": "./dist/jsx-runtime/index.js",
      "types": "./dist/jsx-runtime/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./shared": {
      "import": "./dist/shared.js",
      "types": "./dist/shared.d.ts"
    }
  },
  "scripts": {
    "dev:server": "vite dev",
    "build:watch": "tsc --watch -p .",
