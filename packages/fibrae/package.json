{
  "name": "fibrae",
  "version": "0.2.1",
  "description": "Effect-first JSX renderer with automatic reactivity",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielFGray/fibrae"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "import": "./dist/jsx-runtime/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "import": "./dist/jsx-runtime/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    },
    "./shared": {
      "types": "./dist/shared.d.ts",
      "import": "./dist/shared.js"
    },
    "./router": {
      "types": "./dist/router/index.d.ts",
      "import": "./dist/router/index.js"
    }
  },
  "scripts": {
    "dev:server": "vite dev",
    "build:watch": "tsc --watch -p .",
    "build": "tsc -p .",
    "types:watch": "tsc -p . --noEmit --watch",
    "types:check": "tsc -p . --noEmit"
  },
  "devDependencies": {
    "@effect/language-service": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@effect-atom/atom": "catalog:",
    "effect": "catalog:"
  },
  "optionalDependencies": {
    "@effect/platform-browser": "catalog:",
    "@effect/platform-bun": "catalog:",
    "@effect/platform-node": "catalog:"
  },
  "overrides": {
    "effect": "catalog:"
  }
}
