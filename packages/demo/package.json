  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:server": "vite dev",
    "dev:ssr": "bun server/index.ts",
    "dev:all": "run-p dev:server dev:ssr",
    "client:build": "vite build",
    "build": "run-s types:check client:build",
    "types:watch": "tsc -p . --noEmit --watch",
    "types:check": "tsc -p . --noEmit",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --quiet",
    "ssr": "bun src/demo-ssr.ts",
    "ssr:html": "mkdir -p dist && bun src/demo-ssr.ts > dist/ssr.html && echo 'Wrote dist/ssr.html'"
  },
  "dependencies": {
    "fibrae": "workspace:*",
